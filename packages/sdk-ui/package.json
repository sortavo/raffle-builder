{
  "name": "@sortavo/sdk-ui",
  "version": "1.0.0",
  "description": "Embeddable UI components for Sortavo SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./native": {
      "import": "./dist/native.mjs",
      "require": "./dist/native.js",
      "types": "./dist/native.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "@sortavo/sdk": "*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "tsup": "^8.3.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "sortavo",
    "raffle",
    "rifa",
    "sdk",
    "components",
    "react-native"
  ],
  "license": "MIT"
}
